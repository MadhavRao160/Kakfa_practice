bin/kafka-topics.sh --create \
    --zookeeper localhost:2181 \
    --replication-factor 1 \
    --partitions 1 \
    --topic KeyLessTopic
    
 
cat topics-to-move.json
{"name":"John", "exp":16}
{"name":"Finn", "exp":20}
